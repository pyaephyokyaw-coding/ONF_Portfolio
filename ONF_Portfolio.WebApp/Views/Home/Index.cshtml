@model IEnumerable<ProjectsModel>

@{
    ViewData["Title"] = "Home Page";
}

<h2>My Projects</h2>
@foreach (var p in Model)
{
    <div>
        <h3>@p.Title</h3>
        <p>@p.Description</p>
        <a href="@p.Url">View</a>
    </div>
}

